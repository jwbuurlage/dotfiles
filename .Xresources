URxvt*saveLines: 12000
URxvt.keysym.C-7:               command:\033]710;xft:Terminus:pixelsize=10\007
URxvt.keysym.C-8:               command:\033]710;xft:Terminus:pixelsize=14\007
URxvt.keysym.C-9:               command:\033]710;xft:Terminus:pixelsize=18\007
URxvt.keysym.C-0:               command:\033]710;xft:Terminus:pixelsize=22\007

!URxvt.keysym.C-7:               command:\033]710;xft:Terminus:pixelsize=10\007\033]711;xft:Fixed:pixelsize=10\007
!URxvt.keysym.C-8:               command:\033]710;xft:Terminus:pixelsize=12\007\033]711;xft:Fixed:pixelsize=12\007
!URxvt.keysym.C-9:               command:\033]710;xft:Terminus:pixelsize=14\007\033]711;xft:Fixed:pixelsize=14\007
!URxvt.keysym.C-0:               command:\033]710;xft:Terminus:pixelsize=16\007\033]711;xft:Fixed:pixelsize=16\007 

*scrollBar:        false
!URxvt.font: xft:Terminus:pixelsize=12,xft:Fixed:pixelsize=12
URxvt.font        : xft:DejaVu Sans Mono:pixelsize=12
URxvt.letterSpace : -1
URxvt.internalBorder : 10
urxvt*matcher.button:   1
!urxvt.transparent:    true
!urxvt*shading: 5
urxv*allow_bold:    false
urxvt*tintColor: #CCCCCC
URxvt*foreground: #d5d5d5 
URxvt*background: #1d1d1d
URxvt*cursorColor: #a60800

!XFT
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb

!Black
*color0: #121212
*color8: #2e3436
!Red
*color1: #a35b66
*color9: #ab6b74
!Green
*color2: #99ab6f
*color10: #acb792
!Yellow
*color3: #ca9733
*color11: #ccaa69
!Blue
*color4: #495d6e
*color12: #687987
!Magenta
*color5: #825969
*color13: #977381
!Cyan
*color6: #839191
*color14: #98A4A4
!White
*color7: #e0e0e0
*color15: #e5e5e5
